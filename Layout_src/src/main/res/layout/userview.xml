<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#373737"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:background="@drawable/bg">

        <ImageView
            android:layout_width="228dp"
            android:layout_height="41dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="25dp"
            android:background="@drawable/jhawk_header"></ImageView>

        <Button
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginTop="16dp"
            android:layout_marginLeft="5dp"
            android:background="@drawable/shopping_cart"></Button>

        <Button
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:hint="Log out"
            android:textColorHint="#fff"
            android:textSize="22dp"></Button>

    </LinearLayout>

    <ImageView
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="110dp"
        android:background="@drawable/item_image"
        android:id="@+id/imageView"></ImageView>

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="100dp"


        ></ListView>

    <ImageView
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_below="@id/imageView"
        android:layout_marginTop="17dp"
        android:layout_marginLeft="10dp"
        android:id="@+id/imageView1"
        android:background="@drawable/item_image"
        tools:ignore="DuplicateIds"></ImageView>
    <ImageView
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_below="@id/imageView1"
        android:layout_marginTop="17dp"
        android:layout_marginLeft="10dp"
        android:id="@+id/imageView2"
        android:background="@drawable/item_image"
        tools:ignore="DuplicateIds"></ImageView>
    <ImageView
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_below="@id/imageView2"
        android:layout_marginTop="17dp"
        android:layout_marginLeft="10dp"
        android:id="@+id/imageView3"
        android:background="@drawable/item_image"
        tools:ignore="DuplicateIds">

    </ImageView>
    <ImageView
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_below="@id/imageView3"
        android:layout_marginTop="17dp"
        android:layout_marginLeft="10dp"
        android:id="@+id/imageView4"
        android:background="@drawable/item_image"
        tools:ignore="DuplicateIds">

    </ImageView>
    <ImageView
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_below="@id/imageView4"
        android:layout_marginTop="17dp"
        android:layout_marginLeft="10dp"
        android:id="@+id/imageView5"
        android:background="@drawable/item_image"
        tools:ignore="DuplicateIds">

    </ImageView>
    <ImageView
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_below="@id/imageView5"
        android:layout_marginTop="17dp"
        android:layout_marginLeft="10dp"
        android:id="@+id/imageView6"
        android:background="@drawable/item_image"
        tools:ignore="DuplicateIds">
    </ImageView>
    <ImageView
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_below="@id/imageView6"
        android:layout_marginTop="17dp"
        android:layout_marginLeft="10dp"
        android:id="@+id/imageView7"
        android:background="@drawable/item_image"
        tools:ignore="DuplicateIds">

    </ImageView>
    <ImageView
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_below="@id/imageView7"
        android:layout_marginTop="17dp"
        android:layout_marginLeft="10dp"
        android:id="@+id/imageView8"
        android:background="@drawable/item_image"
        tools:ignore="DuplicateIds">

    </ImageView>

    <ImageView
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_below="@id/imageView8"
        android:layout_marginTop="17dp"
        android:layout_marginLeft="10dp"
        android:id="@+id/imageView9"
        android:background="@drawable/item_image"
        tools:ignore="DuplicateIds">

    </ImageView>
</RelativeLayout>